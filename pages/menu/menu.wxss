.coffee-add-box{
  position: fixed;
  background-color: #ffffff;
  box-shadow:0px -5px 8px #cccccc;
  height: 200rpx;
  width: 100vw;
  left: 0;
  bottom: 100rpx;
  padding: 0 20rpx;
}
.coffee-add-box .ratio-line .ratio-name::before{
  content: ":";
  position: absolute;
  right: 0;
  bottom: 10rpx;
  height: 10rpx;
  width: 5rpx;
}
.coffee-add-box .ratio-line .ratio-name:after {
    content: " ";
    position: absolute;
    left: 10%;
    bottom: 0;
    right: 0;
    height: 1px;
    width: 80%;
    border-bottom: 1px solid #D9D9D9;
    color: #D9D9D9;
    -webkit-transform-origin: 0 100%;
    transform-origin: 0 100%;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
}
.coffee-add-box .ratio-line .ratio-name{
  font-size: 24rpx;
  text-align: center;
  position: relative;
  padding-bottom: 15rpx;
}
.coffee-add-box .ratio-line .ratio-num{
  font-size: 24rpx;
