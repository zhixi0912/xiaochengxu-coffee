.sweet-box{
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin-top:20rpx;

}
.sweet-box .sweet-line {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  height: 8rpx;
  background-color: #e8e8e8;
  position: relative
}
.sweet-box .sweet-line:before {
    content: " ";
    position: absolute;
    width: 40rpx;
    height: 40rpx;
    border-radius: 50%;
    bottom: 0;
    background-color: #e8e8e8;
}
.sweet-box .sweet-line::after {
    content: " ";
    position: absolute;
    width: 30rpx;
    height: 30rpx;
    border-radius: 50%;
    bottom: 0;
    
}
.sweet-box .sweet-line.on::after{
  background-color: #f0c03a;
}
.sweet-box .sweet-line.sweet-1:before{
  left: 0;
  top: 50%;
  margin-top: -20rpx;
}
.sweet-box .sweet-line.sweet-2:before{
  left: 50%;
  top: 50%;
  margin-left: -20rpx;
  margin-top: -20rpx;
}
.sweet-box .sweet-line.sweet-3:before{
  right: 0;
  top: 50%;
  margin-top: -20rpx;
}
.sweet-box .sweet-line.sweet-1:after{
  left: 0;
  top: 50%;
  margin-top: -15rpx;
  margin-left: 5rpx;
}
.sweet-box .sweet-line.sweet-2:after{
  left: 50%;
  top: 50%;
  margin-left: -15rpx;
  margin-top: -15rpx;
}
.sweet-box .sweet-line.sweet-3:after{
  right: 0;
  top: 50%;
  margin-top: -15rpx;
  margin-right: 5rpx;
}
